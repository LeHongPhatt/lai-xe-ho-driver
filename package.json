{
  "name": "gofastbikerbiker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "android": "APP_ENV=local react-native run-android",
    "ios": "APP_ENV=local react-native run-ios --simulator=\"iPhone 14\"",
    "start": "APP_ENV=local react-native start --reset-cache",
    "build:android": "cd android && NODE_ENV=production ./gradlew assembleRelease && cd ..",
    "lint": "eslint --fix ./src/ --ext .js,.jsx,.ts,.tsx",
    "pod": "cd ios && rm -rf Podfile.lock && pod install && cd ..",
    "prepare": "husky install",
    "test": "jest",
    "link-asset": "npx react-native-asset",
    "postinstall": "react-native setup-ios-permissions && yarn run pod",
    "apk": "cd android && NODE_ENV=production &&./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && NODE_ENV=production &&./gradlew clean && ./gradlew bundleRelease && cd .."
  },
